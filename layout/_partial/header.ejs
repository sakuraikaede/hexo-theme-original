<div class="d-flex flex-column align-items-center justify-content-center" style="padding: 30px 20px 15px;">
  <div style="font-size: 2.5em;"><%= config.title %></div>
  <div style="font-size: 1em;color: gray;"><%= config.subtitle %> </div>
  <ul class="nav justify-content-center">
    <% for (name in theme.menu) { %> 
      <li class="nav-item">
        <a class="nav-link" href="<%- url_for(theme.menu[name]) %>"><%= name %></a>
      </li>
  <% } %>
  </ul>
</div>